{
    "version": 2,
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "prisma/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.ts"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "DATABASE_URL": "@database_url",
        "SUPABASE_URL": "@supabase_url",
        "SUPABASE_ANON_KEY": "@supabase_anon_key",
        "SUPABASE_SERVICE_KEY": "@supabase_service_key",
        "JWT_SECRET": "@jwt_secret"
    }
}